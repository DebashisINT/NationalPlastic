                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:contentDescription="@null"
                                android:padding="@dimen/_3sdp"
                                android:layout_marginRight="@dimen/_20sdp"
                                android:src="@drawable/ic_gray_down_arrow_icon" />

                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_1sdp"
                            android:layout_below="@id/tv_party_dropdown"
                            android:layout_marginLeft="@dimen/_10sdp"
                            android:layout_marginRight="@dimen/_25sdp"
                            android:background="@color/light_gray" />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_marginTop="@dimen/_5sdp">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_30sdp"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/tv_frag_add_acti_due_date_time_heading"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_marginLeft="@dimen/_10sdp"
                                android:text="@string/due_date_time"
                                android:gravity="start|center"
                                android:textColor="@android:color/black"
                                android:textSize="@dimen/_12sdp" />

                            <com.breezefieldnationalplastic.widgets.AppCustomTextView
                                android:id="@+id/tv_frag_add_acti_due_date_time_heading_star"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="*"
                                android:textColor="@color/red"
                                android:layout_marginTop="@dimen/_2sdp"
                                android:textSize="@dimen/_15sdp"
                                android:layout_marginLeft="@dimen/_5sdp"/>

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_30sdp"
                            android:orientation="horizontal">

                            <com.breezefieldnationalplastic.widgets.AppCustomTextView
                                android:id="@+id/tv_due_date"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:layout_marginLeft="@dimen/_10sdp"
                                android:hint="Date"
                                android:gravity="start|center"
                                android:textColor="@color/black"
                                android:textSize="@dimen/_13sdp" />

                            <Space
                                android:layout_width="@dimen/_20sdp"
                                android:layout_height="match_parent"/>

                            <com.breezefieldnationalplastic.widgets.AppCustomTextView
                                android:id="@+id/tv_due_time"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:layout_marginLeft="@dimen/_10sdp"
                                android:hint="Time"
                                android:gravity="start|center"
                                android:textColor="@color/black"
                                android:textSize="@dimen/_13sdp" />


                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">
                            <View
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/_1sdp"
                                android:layout_weight="1"
                                android:layout_below="@id/tv_party_dropdown"
                                android:layout_marginLeft="@dimen/_10sdp"
                                android:layout_marginRight="@dimen/_25sdp"
                                android:background="@color/light_gray" />

                            <Space
                                android:layout_width="@dimen/_20sdp"
                                android:layout_height="match_parent"/>
                            <View
                                android:id="@+id/v_due_time_view"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/_1sdp"
                                android:layout_weight="1"
                                android:layout_below="@id/tv_party_dropdown"
                                android:layout_marginLeft="@dimen/_10sdp"
                                android:layout_marginRight="@dimen/_25sdp"
                                android:background="@color/light_gray" />
                        </LinearLayout>



                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"

                        android:layout_marginTop="@dimen/_5sdp">

                        <com.google.android.material.textfield.TextInputLayout
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_30sdp"
                            android:layout_marginLeft="@dimen/_10sdp"
                            android:layout_marginRight="@dimen/_25sdp"
                            android:textColorHint="@color/light_gray"
                            app:hintTextAppearance="@style/TextAppearance.App.TextInputLayout">

                            <com.breezefieldnationalplastic.widgets.AppCustomEditText
                                android:id="@+id/et_attachment"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@android:color/transparent"
                                android:hint="@string/attachment"
                                android:singleLine="true"
                                android:editable="false"
                                android:focusable="false"
                                android:textSize="@dimen/_13sdp"
                                app:theme="@style/EvoEditTextTheme" />
                        </com.google.android.material.textfield.TextInputLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_1sdp"
                            android:layout_below="@id/tv_party_dropdown"
                            android:layout_marginLeft="@dimen/_10sdp"
                            android:layout_marginRight="@dimen/_25sdp"
                            android:background="@color/light_gray" />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"

                        android:layout_marginTop="@dimen/_5sdp">

                        <com.google.android.material.textfield.TextInputLayout
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_30sdp"
                            android:layout_marginLeft="@dimen/_10sdp"
                            android:layout_marginRight="@dimen/_25sdp"
                            android:textColorHint="@color/light_gray"
                            app:hintTextAppearance="@style/TextAppearance.App.TextInputLayout">

                            <com.breezefieldnationalplastic.widgets.AppCustomEditText
                                android:id="@+id/et_photo"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@android:color/transparent"
                                android:hint="@string/photo_without_colon"
                                android:singleLine="true"
                                android:editable="false"
                                android:focusable="false"
                                android:textSize="@dimen/_13sdp"
                                app:theme="@style/EvoEditTextTheme" />
                        </com.google.android.material.textfield.TextInputLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_1sdp"
                            android:layout_below="@id/tv_party_dropdown"
                            android:layout_marginLeft="@dimen/_10sdp"
                            android:layout_marginRight="@dimen/_25sdp"
                            android:background="@color/light_gray" />

                    </LinearLayout>


                </LinearLayout>

            </ScrollView>

            <com.breezefieldnationalplastic.widgets.AppCustomTextView
                android:id="@+id/submit_button_TV"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/shape_custom_border_blue_button"
                android:clickable="true"
                android:focusable="true"
                android:gravity="center"
                android:padding="@dimen/_8sdp"
                android:text="@string/Submit"
                android:textColor="@drawable/selector_text_clor_blue_white"
                android:textSize="@dimen/_14sdp"
                android:layout_marginTop="@dimen/_8sdp"/>

        </LinearLayout>

        <com.pnikosis.materialishprogress.ProgressWheel
            android:id="@+id/progress_wheel"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_gravity="center"
            wheel:matProg_barColor="@color/default_text_color"
            wheel:matProg_progressIndeterminate="true" />

    </FrameLayout>



</LinearLayout>