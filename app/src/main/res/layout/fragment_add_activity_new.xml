                                android:textColor="@android:color/black"
                                android:textSize="@dimen/_12sdp" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="*"
                                android:textColor="@color/red"
                                android:layout_marginTop="@dimen/_2sdp"
                                android:textSize="@dimen/_15sdp"
                                android:layout_marginLeft="@dimen/_5sdp"/>

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_30sdp"
                            android:orientation="horizontal">

                            <com.breezefieldnationalplastic.widgets.AppCustomTextView
                                android:id="@+id/tv_type_dropdown"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:layout_marginLeft="@dimen/_10sdp"
                                android:hint="@string/select_type"
                                android:gravity="start|center"
                                android:textColor="@color/black"
                                android:textSize="@dimen/_13sdp" />

                            <ImageView
                                android:id="@+id/iv_type_dropdown_icon"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:contentDescription="@null"
                                android:padding="@dimen/_3sdp"
                                android:layout_marginRight="@dimen/_20sdp"
                                android:src="@drawable/ic_gray_down_arrow_icon" />

                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_1sdp"
                            android:layout_below="@id/tv_party_dropdown"
                            android:layout_marginLeft="@dimen/_10sdp"
                            android:layout_marginRight="@dimen/_25sdp"
                            android:background="@color/light_gray" />

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/ll_product"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_marginTop="@dimen/_5sdp">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_30sdp"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_marginLeft="@dimen/_10sdp"
                                android:text="@string/enter_product"
                                android:gravity="start|center"
                                android:textColor="@android:color/black"
                                android:textSize="@dimen/_12sdp" />


                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_30sdp"
                            android:orientation="horizontal">

                            <com.breezefieldnationalplastic.widgets.AppCustomTextView
                                android:id="@+id/tv_product_dropdown"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:layout_marginLeft="@dimen/_10sdp"
                                android:hint="@string/select_product"
                                android:gravity="start|center"
                                android:textColor="@color/black"
                                android:textSize="@dimen/_13sdp" />

                            <ImageView
                                android:id="@+id/iv_product_dropdown_icon"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:contentDescription="@null"
                                android:padding="@dimen/_3sdp"
                                android:layout_marginRight="@dimen/_20sdp"
                                android:src="@drawable/ic_gray_down_arrow_icon" />

                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_1sdp"
                            android:layout_below="@id/tv_party_dropdown"
                            android:layout_marginLeft="@dimen/_10sdp"
                            android:layout_marginRight="@dimen/_25sdp"
                            android:background="@color/light_gray" />

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/ll_subject"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"

                        android:layout_marginTop="@dimen/_5sdp">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:layout_marginRight="@dimen/_25sdp">

                            <com.google.android.material.textfield.TextInputLayout
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/_30sdp"
                                android:layout_weight="1"
                                android:layout_marginLeft="@dimen/_10sdp"
                                android:layout_marginRight="@dimen/_25sdp"
                                android:textColorHint="@color/light_gray"
                                app:hintTextAppearance="@style/TextAppearance.App.TextInputLayout">

                                <com.breezefieldnationalplastic.widgets.AppCustomEditText
                                    android:id="@+id/et_subject"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@android:color/transparent"
                                    android:hint="@string/subject"
                                    android:imeOptions="actionNext"
                                    android:inputType="text"
                                    android:singleLine="true"
                                    android:textSize="@dimen/_13sdp"
                                    app:theme="@style/EvoEditTextTheme" />
                            </com.google.android.material.textfield.TextInputLayout>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="*"
                                android:textColor="@color/red"
                                android:layout_marginTop="@dimen/_2sdp"
                                android:textSize="@dimen/_15sdp"
                                android:layout_marginLeft="@dimen/_5sdp"/>

                        </LinearLayout>



                        <View
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_1sdp"
                            android:layout_below="@id/tv_party_dropdown"
                            android:layout_marginLeft="@dimen/_10sdp"
                            android:layout_marginRight="@dimen/_25sdp"
                            android:background="@color/light_gray" />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"

                        android:layout_marginTop="@dimen/_5sdp">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:layout_marginRight="@dimen/_25sdp">

                            <com.google.android.material.textfield.TextInputLayout
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/_30sdp"
                                android:layout_weight="1"
                                android:layout_marginLeft="@dimen/_10sdp"
                                android:layout_marginRight="@dimen/_25sdp"
                                android:textColorHint="@color/light_gray"
                                app:hintTextAppearance="@style/TextAppearance.App.TextInputLayout">

                                <com.breezefieldnationalplastic.widgets.AppCustomEditText
                                    android:id="@+id/et_details"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@android:color/transparent"
                                    android:hint="@string/enter_details"
                                    android:imeOptions="actionNext"
                                    android:inputType="text"
                                    android:singleLine="true"
                                    android:textSize="@dimen/_13sdp"
                                    app:theme="@style/EvoEditTextTheme" />
                            </com.google.android.material.textfield.TextInputLayout>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="*"
                                android:textColor="@color/red"
                                android:layout_marginTop="@dimen/_2sdp"
                                android:textSize="@dimen/_15sdp"
                                android:layout_marginLeft="@dimen/_5sdp"/>

                        </LinearLayout>



                        <View
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_1sdp"
                            android:layout_below="@id/tv_party_dropdown"
                            android:layout_marginLeft="@dimen/_10sdp"
                            android:layout_marginRight="@dimen/_25sdp"
                            android:background="@color/light_gray" />

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/ll_duration"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_marginTop="@dimen/_5sdp">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_30sdp"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_marginLeft="@dimen/_10sdp"
                                android:text="@string/duration_hh_mm"
                                android:gravity="start|center"
                                android:textColor="@android:color/black"
                                android:textSize="@dimen/_12sdp" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="*"
                                android:textColor="@color/red"
                                android:layout_marginTop="@dimen/_2sdp"
                                android:textSize="@dimen/_15sdp"
                                android:layout_marginLeft="@dimen/_5sdp"/>

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/_30sdp"
                            android:orientation="horizontal"
                            android:layout_marginLeft="@dimen/_10sdp">

                            <com.breezefieldnationalplastic.widgets.AppCustomEditText
                                android:id="@+id/et_hrs"
                                android:layout_width="@dimen/_80sdp"
                                android:layout_height="match_parent"
                                android:background="@android:color/transparent"
                                android:hint="Hrs"
                                android:maxLength="2"
                                android:imeOptions="actionNext"
                                android:inputType="number"
                                android:gravity="center"
                                android:textColor="@color/black"
                                android:textSize="@dimen/_13sdp" />

                            <Space
                                android:layout_width="@dimen/_20sdp"
                                android:layout_height="match_parent"/>

                            <com.breezefieldnationalplastic.widgets.AppCustomEditText
                                android:id="@+id/et_mins"
                                android:layout_width="@dimen/_80sdp"
                                android:layout_height="match_parent"
                                android:layout_marginLeft="@dimen/_10sdp"
                                android:hint="Mins"
                                android:background="@android:color/transparent"
                                android:maxLength="2"
                                android:imeOptions="actionNext"
                                android:inputType="number"
                                android:gravity="center"
                                android:textColor="@color/black"
                                android:textSize="@dimen/_13sdp" />


                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:layout_marginLeft="@dimen/_10sdp">
                            <View
                                android:layout_width="@dimen/_80sdp"
                                android:layout_height="@dimen/_1sdp"
                                android:layout_weight="1"
                                android:layout_below="@id/tv_party_dropdown"

                                android:layout_marginRight="@dimen/_25sdp"
                                android:background="@color/light_gray" />

                            <Space
                                android:layout_width="@dimen/_20sdp"
                                android:layout_height="match_parent"/>
                            <View
                                android:layout_width="@dimen/_80sdp"
                                android:layout_height="@dimen/_1sdp"
                                android:layout_weight="1"
                                android:layout_below="@id/tv_party_dropdown"
                                android:layout_marginRight="@dimen/_25sdp"
                                android:background="@color/light_gray" />
                        </LinearLayout>



                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/ll_priority"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_30sdp"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_marginLeft="@dimen/_10sdp"
                                android:text="@string/priority"
                                android:gravity="start|center"
                                android:textColor="@android:color/black"
                                android:textSize="@dimen/_12sdp" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="*"
                                android:textColor="@color/red"
                                android:layout_marginTop="@dimen/_2sdp"
                                android:textSize="@dimen/_15sdp"
                                android:layout_marginLeft="@dimen/_5sdp"/>

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_30sdp"
                            android:orientation="horizontal">

                            <com.breezefieldnationalplastic.widgets.AppCustomTextView
                                android:id="@+id/tv_priority_dropdown"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:layout_marginLeft="@dimen/_10sdp"
                                android:hint="@string/select_priority"
                                android:gravity="start|center"
                                android:textColor="@color/black"
                                android:textSize="@dimen/_13sdp" />

                            <ImageView
